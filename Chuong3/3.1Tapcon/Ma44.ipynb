   "source": [
    "def power_set ( a : list ):\n",
    "    if len( a ) == 0:\n",
    "        return [[]]\n",
    "    P = [[]]\n",
    "    for s in power_set ( a [1:]) :\n",
    "        s = [ a [0]] + s\n",
    "        P . append ( s)\n",
    "    for s in power_set ( a [1:]) [1:]:\n",
    "        P . append ( s)\n",
    "    return P\n",
    "\n",
    "power_set([1,2,3,4])"
   ]
  }
