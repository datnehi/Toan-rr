    "a = [6 , 4 , 2 , 7]\n",
    "b = [5 , 3 , 7 , 4]\n",
    "base = 8\n",
    "k = len( a )\n",
    "r = 0\n",
    "s = [0] * ( k +1)\n",
    "for i in range ( k ) :\n",
    "    t = a [ i] + b [ i ] + r\n",
    "    s [ i ] = t % base\n",
    "    r = t // base\n",
    "s [ k ] = r\n",
    "s\n"
   ]
