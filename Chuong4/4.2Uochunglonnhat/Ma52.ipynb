    "def gcdex (a , b ) :\n",
    "    x0 , y0 = 1 , 0\n",
    "    x1 , y1 = 0 , 1\n",
    "    while b != 0:\n",
    "        q = a // b\n",
    "        a , b = b , a % b\n",
    "        x = x0 - x1 * q\n",
    "        y = y0 - y1 * q\n",
    "        x0 , y0 = x1 , y1\n",
    "        x1 , y1 = x , y\n",
    "    return x0 , y0 , a\n",
    "gcdex (287 , 291)"
   ]
