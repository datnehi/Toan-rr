    "def next_combinations (n , a ) :\n",
    "    r = len ( a )\n",
    "    i = r - 1\n",
    "    while i >= 0 and a [ i ] == n - r + ( i + 1) :\n",
    "        i -= 1\n",
    "    if i == -1:\n",
    "        return None\n",
    "    return a [: i ] + [ a [ i ] + j for j in range (1 , r - i +1) ]\n",
    "next_combinations (6 , [1 , 2 , 5 , 6])\n"
   ]
  }
