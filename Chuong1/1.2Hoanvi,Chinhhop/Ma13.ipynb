    "def binary_strs ( n ) :\n",
    "    if n ==1:\n",
    "        return ['0', '1']\n",
    "    A = []\n",
    "    for s in binary_strs (n -1) :\n",
    "        A . append ('0' + s )\n",
    "    for s in binary_strs (n -1) :\n",
    "        A . append ('1' + s )\n",
    "    return A\n",
