   "source": [
    "def next_bin_str ( a ) :\n",
    "    n = len ( a )\n",
    "    i = n - 1\n",
    "    while i >=0 and a [ i ] == 1:\n",
    "        i -= 1\n",
    "    if i == -1:\n",
    "        return None\n",
    "    for j in range (i , n) :\n",
    "        a [ j ] = 1 - a [ j ]\n",
    "    return a\n",
    "a = [1 , 0 , 0 , 0 , 1 , 0 , 0 , 1 , 1 , 1]\n",
    "next_bin_str ( a )\n"
   ]
  }
