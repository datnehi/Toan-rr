   "source": [
    "def solutions (n , r ) :\n",
    "    if n == 1:\n",
    "        return [[ r ]]\n",
    "    S = []\n",
    "    for i in range ( r +1) :\n",
    "        for s in solutions (n -1 , r - i ) :\n",
    "            s = [ i ] + s\n",
    "            S . append ( s )\n",
    "    return S\n",
    "solutions (3 , 4)"
   ]
  }
