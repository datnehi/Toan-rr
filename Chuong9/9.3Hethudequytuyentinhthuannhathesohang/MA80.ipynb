   "source": [
    "from sympy import *\n",
    "n = symbols ('n')\n",
    "a = symbols ('a', cls = Function )\n",
    "ans = rsolve (\n",
    "    a ( n ) - 2* a (n -2) ,\n",
    "    a ( n ) ,\n",
    "    { a (1) : 1 , a (2) : 2}\n",
    ")\n",
    "ans\n",
    "k = symbols ('k', integer = True )\n",
    "ans . subs (n , 2* k ) . simplify ()\n",
    "ans . subs (n , 2* k +1) . simplify ()"
   ]
  }
