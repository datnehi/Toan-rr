   "source": [
    "def hanoi_tower (n , A , B , C ) :\n",
    "    if n == 1:\n",
    "        return [[1 , A , B ]]\n",
    "    return hanoi_tower (n -1 , A , C , B ) + [[ n , A , B ]] + hanoi_tower (n -1 , C ,\n",
    "    B , A )\n",
    "hanoi_tower (3 , 'A','B', 'C')"
   ]
