    "def BubleSort ( x ) :\n",
    "    n = len ( x )\n",
    "    if n == 1:\n",
    "        return x\n",
    "    for i in range (n -1 , 0 , -1) :\n",
    "        if x [ i ] < x [i -1]:\n",
    "            x [ i ] , x [i -1] = x [i -1] , x [ i ]\n",
    "    return[x[0]]+BubleSort(x[1:])\n",
    "BubleSort ([7 , 9 , 2 , 5 , 8])\n"
   ]
  }
